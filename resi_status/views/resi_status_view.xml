<openerp>
	<data>
		<!-- Promo -->

		<record id="resi_status_tree" model="ir.ui.view">
			<field name="name">resi.status.tree</field>
			<field name="model">resi.status</field>
			<field name="arch" type="xml">
				<tree string="resi">
					<field name="name"/>
					<field name="recipient"/>
					<field name="account_analytic_id"/>
					<field name="price_cod"/>
					<field name="price_package"/>
					<field name="sigesit"/>
					<field name="internal_status"/>
					<field name="external_status"/>
					<field name="status_retur"/>
				</tree>
			</field>
		</record>

		<record id="resi_status_form" model="ir.ui.view">
			<field name="name">resi.status.form</field>
			<field name="model">resi.status</field>
			<field name="arch" type="xml">
				<form string="Resi">
					<group>
						<group>
							<field name="name"/>
							<field name="recipient"/>
							<field name="account_analytic_id"/>
							<field name="price_cod"/>
							<field name="price_package"/>
						</group>
						<group>
							<field name="sigesit"/>
							<field name="internal_status"/>
							<field name="external_status"/>
							<field name="status_retur"/>
						</group>
					</group>
				</form>
			</field>
		</record>

		<record id="resi_status_search" model="ir.ui.view">
			<field name="name">resi.status.search</field>
			<field name="model">resi.status</field>
			<field name="arch" type="xml">
				<search string="Resi">
					<field name="name"/>
					<field name="account_analytic_id"/>
					<field name="sigesit"/>
					<field name="internal_status"/>
					<field name="external_status"/>
					<field name="status_retur"/>
				</search>
			</field>
		</record>
		
		<record id="action_resi_status" model="ir.actions.act_window">
			<field name="name">Resi</field>
			<field name="res_model">resi.status</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field eval="False" name="view_id"/>
			<field name="domain">[]</field>
			<field name="context">{}</field>
			<field name="search_view_id" ref="resi_status_search"/>
			<field name="help" type="html">
			  <p class="oe_view_nocontent_create">
				Click to create a Resi Status.</p>
			</field>
		</record>


		<menuitem id="menu_resi_status" name="Resi" parent="" sequence="7"/>
		<menuitem id="menu_resi_status1" name="Resi" parent="menu_resi_status" sequence="1"/>
		<menuitem id="menu_resi_status2" name="Resi Status" action="action_resi_status" parent="menu_resi_status1" sequence="1"/>
	</data>
</openerp>