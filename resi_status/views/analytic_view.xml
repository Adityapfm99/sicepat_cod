<openerp>
	<data>
		<record id="account_analytic_cod_admin" model="ir.ui.view">
			<field name="name">account.analytic.cod.admin</field>
			<field name="model">account.analytic.account</field>
			<field name="inherit_id" ref="analytic.view_account_analytic_account_form"/>
			<field name="arch" type="xml">
				<xpath expr="//form/sheet/notebook/page[@string='Contract Information']" position="after">
					<page string="COD Configuration">
						<group name="account_cod" string="User Admin Cabang">
							<field name="user_admin_cabang" colspan="4" nolabel="1"/>
						</group>
					</page>
				</xpath>
			</field>
		</record>
	</data>
</openerp>